get-preferences-menu | click
get-window Preferences | get-tree | select Sirius

with [get-window Preferences] {
    with [get-group Refresh] {
        get-button "Do refresh on representation opening" | uncheck
        get-button "Automatic Refresh" | uncheck
    }
    get-button Apply | click
    get-button OK | click
}

get-view "Capella Project Explorer" | get-tree | select "test/test.aird" | get-menu "Model Detachment" | click
get-editor "Detachment" | get-tab-folder | get-tab-item "Viewpoints Tree Page" | click
get-editor "Detachment" | get-section "Viewpoints Hierarchy" | get-tree | get-item "Basic Price" | uncheck
get-editor "Detachment" | get-section "Viewpoints Hierarchy" | get-tree | get-item "Basic Performance" | uncheck
get-editor "Detachment" | get-section "Viewpoints Hierarchy" | get-tree | get-item "Basic Mass" | uncheck
get-editor "Detachment" | get-section "Viewpoints Hierarchy" | get-tree | get-property itemCount | equals 3 
    | verify-true
get-editor "Detachment" | get-button "Perform Detachment" | click
get-window "Warning..." | get-button Yes | click
get-window "Detachment..." | get-button OK | click
get-view "Capella Project Explorer" | get-tree | select "test/test.aird" | get-menu "Model Detachment" | click
get-editor "Detachment" | get-tab-folder | get-tab-item "Viewpoints Tree Page" | click
get-editor "Detachment" | get-section "Viewpoints Hierarchy" | get-tree | get-property itemCount | equals 0 
    | verify-true
get-editor "Detachment" | close
with [get-view "Capella Project Explorer" | get-tree] {
    select "test/test.aird" | get-menu "Open Session" | click
    select "test/test.aird/test/Physical Architecture/Physical System/\\[PAB\\] Physical System - Physical Architecture Blank" 
        | double-click
}
get-tab-folder -index 2 | get-property activePage | equals "[PAB] Physical System - Physical Architecture Blank" 
    | verify-true

get-view "Capella Project Explorer" | get-tree | select "test/test.aird" | get-menu Close | click

