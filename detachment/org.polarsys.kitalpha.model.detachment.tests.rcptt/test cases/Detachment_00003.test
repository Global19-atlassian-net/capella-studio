get-preferences-menu | click
get-window Preferences | get-tree | select Sirius

with [get-window Preferences] {
    with [get-group Refresh] {
        get-button "Do refresh on representation opening" | uncheck
        get-button "Automatic Refresh" | uncheck
    }
    get-button Apply | click
    get-button OK | click
}

with [get-view "Capella Project Explorer" | get-tree] {
    select "test/test.aird" | double-click
    select "test/test.aird/test/Physical Architecture/Physical System/\\[PAB\\] Physical System - Physical Architecture Blank" 
        | double-click
}
with [get-editor "[PAB] Physical System - Physical Architecture Blank" | get-diagram -index 1 | get-edit-part 
    -name "[PAB] Physical System - Physical Architecture Blank" | get-edit-part -name "0 - Max: 0" | get-edit-part 
    -className SquareEditPart | get-figure "0/0"] {
    get-property "getSubStringText()" | equals "0 - Max: 0" | verify-true
    get-property "isVisible()" | equals true | verify-true
}

