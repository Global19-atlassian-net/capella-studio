get-view "Project Explorer" | get-tree | get-menu "New/Other..." | click
with [get-window New] {
    get-editbox -after [get-label "Wizards:"] | set-text arch
    get-tree | select "Kitalpha/Architecture description/Architecture Framework DSL Project"
    get-button "Next >" | click
}
with [get-window "New Architecture Framework"] {
    get-editbox -after [get-label "Short name :"] | set-text security4Capella
    get-list | select security security4Capella
    get-button ">" | click
    get-button "Next >" | click
    get-button Finish | click
}
get-view "Project Explorer" | get-tree | get-property itemCount | equals 3 | verify-true
get-view "Project Explorer" | get-tree 
    | select "org.polarsys.kitalpha.af.security4capella.afdsl/model/security4capella.afdesc" 
    | get-menu "Architecture Framework DSL/Generate Architecture Framework" | click
with [get-view "Project Explorer" | get-tree] {
    get-item "org.polarsys.kitalpha.af.security4capella.afdsl" | get-property caption 
        | equals "org.polarsys.kitalpha.af.security4capella.afdsl" | verify-true
    get-item "org.polarsys.kitalpha.af.security4capella.feature" | get-property caption 
        | equals "org.polarsys.kitalpha.af.security4capella.feature" | verify-true
    get-item "org.polarsys.kitalpha.vp.security.model" | get-property caption 
        | equals "org.polarsys.kitalpha.vp.security.model" | verify-true
    get-item "org.polarsys.kitalpha.vp.security.vpdsl" | get-property caption 
        | equals "org.polarsys.kitalpha.vp.security.vpdsl" | verify-true
}
with [get-view "Project Explorer" | get-tree] {
    get-item "org.polarsys.capella.vp.security4capella.vpdsl" | get-property caption 
        | equals "org.polarsys.capella.vp.security4capella.vpdsl" | verify-true
    get-item "org.polarsys.capella.vp.security4capella.model" | get-property caption 
        | equals "org.polarsys.capella.vp.security4capella.model" | verify-true
}
