get-view "Project Explorer" | get-tree
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/model/QualityAssessment.spec.vptext" 
    | get-menu "Open With/Vptext Specification Editor" | click
    
with [get-editor "QualityAssessment.spec.vptext" | get-text-viewer] {
    set-caret-pos 11 32
    key-type Enter
    key-type "M1+SPACE"
}

get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select "UI - New User Interface" 
    | click -default
get-window "Viewpoint UI File Creation Wizard" | get-button Finish | click
with [get-editor "QualityAssessment.ui.vptext" | get-text-viewer] {
    set-caret-pos 10 5
    key-type "M1+SPACE"
}
get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table 
    | select "UI - Generate User Interface for all classes" | click -default
with [get-editor "QualityAssessment.ui.vptext" | get-text-viewer] {
    key-type "M1+M2+f"
    key-type "M1+s"
}
