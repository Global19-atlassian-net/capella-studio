get-view "Project Explorer" | get-tree
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/model/QualityAssessment.spec.vptext" 
    | get-menu "Open With/Vptext Specification Editor" | click
    
with [get-editor "QualityAssessment.spec.vptext" | get-text-viewer] {
	set-caret-pos 11 32
    key-type Enter
    key-type "M1+SPACE"
}
get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select "Services - New Services" 
    | click -default
get-window "Viewpoint Services File Creation Wizard" | get-button Finish | click

// Get properties specification
get-view "Project Explorer" | get-tree 
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/test-artefact/QualityAssessment.services" | double-click
with [get-editor "QualityAssessment.services" | get-text-viewer] {
    set-caret-pos 18 1
    select-range 18 1 20 2
    get-menu Copy | click
}

get-view "Project Explorer" | get-tree 
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/model/QualityAssessment.services.vptext" | double-click
with [get-editor "QualityAssessment.services.vptext" | get-text-viewer] {
    set-caret-pos 10 1
    select-range 9 1 13 2
    //hover-text 14 18
    // key-type BackSpace
    key-type "M1+v"
}
get-button "Save All (M1+M2+S)" | click

// Get Rules specification
get-view "Project Explorer" | get-tree 
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/test-artefact/QualityAssessment.services" | double-click
with [get-editor "QualityAssessment.services" | get-text-viewer] {
    set-caret-pos 9 1
    select-range 9 1 12 2
    get-menu Copy | click
}

get-view "Project Explorer" | get-tree 
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/model/QualityAssessment.services.vptext" | double-click
with [get-editor "QualityAssessment.services.vptext" | get-text-viewer] {
    set-caret-pos 11 2
    key-type Enter
    key-type "M1+v"
}
get-button "Save All (M1+M2+S)" | click

// Get Services specification
get-view "Project Explorer" | get-tree 
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/test-artefact/QualityAssessment.services" | double-click
with [get-editor "QualityAssessment.services" | get-text-viewer] {
    set-caret-pos 13 1
    select-range 13 1 15 2
    get-menu Copy | click
}

get-view "Project Explorer" | get-tree 
    | select "org.polarsys.capella.vp.qualityassessment.vpdsl/model/QualityAssessment.services.vptext" | double-click
with [get-editor "QualityAssessment.services.vptext" | get-text-viewer] {
    set-caret-pos 15 2
    key-type Enter
    key-type "M1+v"
}
get-button "Save All (M1+M2+S)" | click


// Close Artefact Editor
get-tab-folder | get-tab-item "QualityAssessment.services" | click
with [get-editor "QualityAssessment.services"] {
    click
    close
}

